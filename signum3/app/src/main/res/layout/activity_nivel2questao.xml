<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nivel1questao">

    <TextView
        android:id="@+id/textView2"
        android:layout_width="279dp"
        android:layout_height="68dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:background="#E91E63"
        android:onClick="nivel3"
        android:text="enviar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup2" />


    <RadioGroup
        android:id="@+id/radioGroup2"
        android:layout_width="350dp"
        android:layout_height="223dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <RadioGroup
            android:layout_width="342dp"
            android:layout_height="161dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="488dp">

            <RadioButton
                android:id="@+id/radioButton18"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:text="figura 1" />

            <RadioButton
                android:id="@+id/radioButton16"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:text="figura 2" />

        </RadioGroup>

        <RadioGroup
            android:layout_width="342dp"
            android:layout_height="91dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="655dp">

            <RadioButton
                android:id="@+id/radioButton19"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:text="figura 3" />

            <RadioButton
                android:id="@+id/radioButton20"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:text="figura 4" />
        </RadioGroup>


    </RadioGroup>

    <VideoView
        android:id="@+id/videoView4"
        android:layout_width="118dp"
        android:layout_height="118dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <VideoView
        android:id="@+id/videoView5"
        android:layout_width="118dp"
        android:layout_height="118dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.373"
        app:layout_constraintStart_toEndOf="@+id/videoView4"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <VideoView
        android:id="@+id/videoView6"
        android:layout_width="118dp"
        android:layout_height="118dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/videoView4" />

    <VideoView
        android:id="@+id/videoView7"
        android:layout_width="118dp"
        android:layout_height="118dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.373"
        app:layout_constraintStart_toEndOf="@+id/videoView6"
        app:layout_constraintTop_toBottomOf="@+id/videoView5" />


</androidx.constraintlayout.widget.ConstraintLayout>