<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".questao2conjuntos">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="279dp"
        android:layout_height="68dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.465"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/button9"
        android:layout_width="355dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:background="@drawable/roundcornerred"
        android:onClick="questao3"
        android:text="enviar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/radioGroup"
        app:layout_constraintVertical_bias="0.359" />


    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="350dp"
        android:layout_height="223dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3">

        <RadioGroup
            android:id="@+id/NameRadioGroupe2"
            android:layout_width="342dp"
            android:layout_height="161dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="488dp">

            <RadioButton
                android:id="@+id/radioButton25"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:text="figura 1"
                android:onClick="oneRadioButtonClicked"/>

            <RadioButton
                android:id="@+id/radioButton26"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:text="figura 2"
                android:onClick="oneRadioButtonClicked"/>

        </RadioGroup>

        <RadioGroup
            android:id="@+id/NameRadioGroupe1"
            android:layout_width="342dp"
            android:layout_height="91dp"
            android:orientation="horizontal"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="655dp">

            <RadioButton
                android:id="@+id/radioButton27"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:text="figura 3"
                android:onClick="oneRadioButtonClicked"/>

            <RadioButton
                android:id="@+id/radioButton28"
                android:layout_width="50dp"
                android:layout_height="44dp"
                android:layout_weight="1"
                android:text="figura 4"
                android:onClick="oneRadioButtonClicked"/>
        </RadioGroup>


    </RadioGroup>

    <VideoView
        android:id="@+id/videoView9"
        android:layout_width="118dp"
        android:layout_height="118dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <VideoView
        android:id="@+id/videoView10"
        android:layout_width="118dp"
        android:layout_height="118dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.373"
        app:layout_constraintStart_toEndOf="@+id/videoView9"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <VideoView
        android:id="@+id/videoView11"
        android:layout_width="118dp"
        android:layout_height="118dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="40dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/videoView9" />

    <VideoView
        android:id="@+id/videoView12"
        android:layout_width="118dp"
        android:layout_height="118dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.373"
        app:layout_constraintStart_toEndOf="@+id/videoView11"
        app:layout_constraintTop_toBottomOf="@+id/videoView10" />

</androidx.constraintlayout.widget.ConstraintLayout>